{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "hooks": {
        "prebuild": "sed -i \"s/__GA_ID__/$GA_ID/g\" index.html"
    }
  }